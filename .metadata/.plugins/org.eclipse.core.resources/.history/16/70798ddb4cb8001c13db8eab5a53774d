#include <stdio.h>
#include <stdlib.h>
#include "menuopciones.h"
#define EXIT 6

int main(void) {
	setbuf(stdout, NULL);
	int opcion;
	float kms;
	//int aerolineas;
	//int latam;

	do{

		opcion = MenuOpciones();

		switch(opcion)
		{
			case 1:
				kms = IngresarKMS("Ingrese cantidad de kilometros");
		}

	}while(opcion != EXIT);


	return EXIT_SUCCESS;
}
